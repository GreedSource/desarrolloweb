
@{
    ViewData["Title"] = "Calificación de páginas";
    Layout = "~/Views/Shared/_MasterView.cshtml";
}

    <div class="text-center">
        <h1 class="display-4">Ingresa el nombre de la pagina con su calificacion</h1>
        <form method="post" enctype="multipart/form-data" asp-controller="Home" asp-action="Page_view">
            <div class="form-group">
                <label for="nombre">Ingrese el nombre de la pagina</label>
                <input type="text" class="form-control" name="nombre" id="nombre">
            </div>
            <div class="form-group">
                <label for="">Ingrese la calificacion</label>
                <select name="calificacion" class="form-control">
                    <option value="1">1</option>
                    <option value="2">2</option>
                    <option value="3">3</option>
                    <option value="4">4</option>
                    <option value="5">5</option>

                </select>
            </div>
            <button type="submit" class="btn btn-primary">Registrar</button>
        </form>

    </div>

@section js{ 

